{
	"name": "bfinlay/laravel-excel-seeder",
	"description": "Seed the database with Laravel using Excel, XLSX, XLS, CSV, ODS, Gnumeric, XML, HTML, SLK files",
	"homepage": "https://github.com/bfinlay/laravel-excel-seeder",
	"authors": [
		{
			"name": "Brion Finlay"
		}
	],
	"keywords": [ "laravel", "csv", "excel", "seed", "seeds", "seeder", "seeding", "open", "libre", "office", "openoffice", "libreoffice"],
	"license": "MIT",
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"php": ">=7.1.3",
		"laravel/framework": ">=5.8",
		"phpoffice/phpspreadsheet": "~1.7",
		"doctrine/dbal": "^2.6|^3.0",
		"composer/semver": "*"
	},
	"repositories-disabled": [
		{
			"type": "path",
			"url": "../laravel-excel-seeder-test-data"
		},
		{
			"type": "path",
			"url": "~/development/laravel-expression-grammar"
		}
	],
	"require-dev": {
		"orchestra/testbench": "*",
		"orchestra/testbench-core": "^3.0|^4.0|^5.0|~6.48.0|~7.41.0|~8.22.0|^9.0|^10.0",
		"bfinlay/laravel-excel-seeder-test-data": "3.4.1",
		"angel-source-labs/laravel-expression-grammar": "*"
	},
	"autoload": {
		"psr-4": {
			"bfinlay\\SpreadsheetSeeder\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"bfinlay\\SpreadsheetSeeder\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"bfinlay\\SpreadsheetSeeder\\SpreadsheetSeederServiceProvider"
			]
	    }
	}
}
